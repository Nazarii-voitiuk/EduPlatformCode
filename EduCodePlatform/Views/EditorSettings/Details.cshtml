@model EduCodePlatform.Models.EditorSetting

@{
    ViewData["Title"] = "Деталі Editor Setting";
}

<h2>@ViewData["Title"]</h2>

<div class="card">
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-3">ID</dt>
            <dd class="col-sm-9">@Model.EditorSettingId</dd>

            <dt class="col-sm-3">UserId</dt>
            <dd class="col-sm-9">@Model.UserId</dd>

            <dt class="col-sm-3">Theme</dt>
            <dd class="col-sm-9">@Model.Theme</dd>

            <dt class="col-sm-3">TabSize</dt>
            <dd class="col-sm-9">@Model.TabSize</dd>
        </dl>
    </div>
</div>

<div class="mt-3">
    <a asp-action="Edit" asp-route-id="@Model.EditorSettingId" class="btn btn-warning">Редагувати</a>
    <a asp-action="Index" class="btn btn-secondary">Назад до списку</a>
</div>
